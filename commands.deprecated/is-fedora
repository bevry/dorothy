#!/usr/bin/env bash

function is_fedora() (
	source "$DOROTHY/sources/bash.bash"
	dorothy-warnings add --code='is-fedora' --bold=' has been deprecated in favor of ' --code='is-system --fedora'
	is-system --fedora || return $?
)

# fire if invoked standalone
if [[ $0 == "${BASH_SOURCE[0]}" ]]; then
	is_fedora "$@"
fi
