#!/usr/bin/env bash

function fs_dirname() (
	source "$DOROTHY/sources/bash.bash"
	dorothy-warnings crash --code='fs-dirname' --bold=' has been deprecated in favor of ' --code='fs-path --absolute --parents'
)

# fire if invoked standalone
if [[ $0 == "${BASH_SOURCE[0]}" ]]; then
	fs_dirname "$@"
fi
