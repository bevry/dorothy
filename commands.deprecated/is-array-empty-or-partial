#!/usr/bin/env bash

# is the array partially empty?

# pass if any value is empty
# failure if all values are truthy

function is_array_empty_or_partial() (
	source "$DOROTHY/sources/bash.bash"
	dorothy-warnings crash --code='is-array-empty-or-partial' --bold=' has been deprecated in favor of ' --code='is-whitespace' --bold=' and ' --code='is-not-whitespace'
)

# fire if invoked standalone
if [[ $0 == "${BASH_SOURCE[0]}" ]]; then
	is_array_empty_or_partial "$@"
fi
