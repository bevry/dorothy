#!/usr/bin/env bash

function get_file() (
	source "$DOROTHY/sources/bash.bash"
	dorothy-warnings add --code='get-file' --bold=' has been deprecated in favor of ' --code='fs-type'

	# =====================================
	# Action

	fs-type "$@"
)

# fire if invoked standalone
if [[ $0 == "${BASH_SOURCE[0]}" ]]; then
	get_file "$@"
fi
