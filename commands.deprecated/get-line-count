#!/usr/bin/env bash

function get_line_count() (
	source "$DOROTHY/sources/bash.bash"
	dorothy-warnings crash --code='get-line-count' --bold=' has been deprecated in favor of ' --code='echo-count-lines'
)

# fire if invoked standalone
if [[ $0 == "${BASH_SOURCE[0]}" ]]; then
	get_line_count "$@"
fi
