#!/usr/bin/env bash

function get_min() (
	source "$DOROTHY/sources/bash.bash"
	dorothy-warnings crash --code='get-min' --bold=' has been deprecated in favor of ' --code='echo-min'
	# ^ their behaviour is different, however if you want the actual behaviour of this, then you should do it manually, as there are likely hidden quirks with the `sort` approach
)

# fire if invoked standalone
if [[ $0 == "${BASH_SOURCE[0]}" ]]; then
	get_min "$@"
fi
