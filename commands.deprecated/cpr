#!/usr/bin/env bash

function cpr_() (
	source "$DOROTHY/sources/bash.bash"
	dorothy-warnings add --code='cpr' --bold=' has been renamed to ' --code='fs-copy'

	# =====================================
	# Action

	fs-copy "$@"
)

# fire if invoked standalone
if [[ $0 == "${BASH_SOURCE[0]}" ]]; then
	cpr_ "$@"
fi
