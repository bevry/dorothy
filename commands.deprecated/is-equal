#!/usr/bin/env bash

function is_equal() (
	source "$DOROTHY/sources/bash.bash"
	dorothy-warnings crash --code='is-equal' --bold=' has been deprecated in favor of ' --code='test "$a" = "$b"' --bold=' or ' --code='is-needle --needle="$a" -- "$b"'
)

# fire if invoked standalone
if [[ $0 == "${BASH_SOURCE[0]}" ]]; then
	is_equal "$@"
fi
