#!/usr/bin/env bash

function get_release() (
	source "$DOROTHY/sources/bash.bash"
	dorothy-warnings add --code='get-release' --bold=' has been deprecated in favor of ' --code='get-system'
	get-system "$@"
)

# fire if invoked standalone
if [[ $0 == "${BASH_SOURCE[0]}" ]]; then
	get_release "$@"
fi
