#!/usr/bin/env bash

function echo_color_enabled() (
	source "$DOROTHY/sources/bash.bash"
	dorothy-warnings crash --code='echo-color-enabled' --bold=' has been deprecated in favor of ' --code='get-terminal-color-support'
)

# fire if invoked standalone
if [[ $0 == "${BASH_SOURCE[0]}" ]]; then
	echo_color_enabled "$@"
fi
