#!/usr/bin/env bash
source "$DOROTHY/sources/arrays.bash"
source "$DOROTHY/sources/strict.bash"
if test "$ARRAYS" = 'no'; then
	exit 1
fi

# usage:
# mapfile -t values < <(array-odds "${tuples[@]}")

mapfile -t args < <(echo-lines "$@")  # split jumbled arguments
for i in "${!args[@]}"; do
	if is-odd "$i"; then
		echo "${args[$i]}"
	fi
done
