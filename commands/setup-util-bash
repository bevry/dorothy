#!/usr/bin/env bash
source "$DOROTHY/sources/strict.bash"

env NAME='bash' CLI='bash' \
	APK='bash' \
	APT='bash' \
	BREW='bash' \
	PACMAN='bash' \
	PAMAC='bash' \
	setup-util

# no need to do fancy checks, as there is no failure if it is already present
env NAME='Bash Completions' \
	APK=bash-completion \
	APT=bash-completion \
	BREW=bash-completion \
	PACMAN=bash-completion \
	setup-util
