#!/usr/bin/env bash
source "$DOROTHY/sources/strict.bash"

# attempt default, ssh, https
("$DOROTHY/commands/git-protocol-apply" "${1-}" && silent "$DOROTHY/commands/is-ssh-working") || \
("$DOROTHY/commands/git-protocol-apply" "${1-}" ssh && silent "$DOROTHY/commands/is-ssh-working") || \
("$DOROTHY/commands/git-protocol-apply" "${1-}" https)
