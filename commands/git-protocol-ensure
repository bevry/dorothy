#!/usr/bin/env bash
source "$DOROTHY/sources/strict.bash"

# attempt default, ssh, https
("$DOROTHY/commands/git-protocol-apply" "${1-}" && "$DOROTHY/commands/ssh-test") || \
("$DOROTHY/commands/git-protocol-apply" "${1-}" ssh && "$DOROTHY/commands/ssh-test") || \
("$DOROTHY/commands/git-protocol-apply" "${1-}" https)
