#!/usr/bin/env bash
source "$DOROTHY/sources/strict.bash"

# checks if the subsequent arguments exists inside the first argument

# convert to lower case
haystack="${1@L}"

for needle in "${@:2}"; do
	# convert to lowercase
	needle="${needle@L}"

	# check if it is present
	if [[ "$haystack" = *"$needle"* ]]; then
		exit
	fi
done

exit 1
