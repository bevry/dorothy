	BREW='glab' \
	WINGET='glab' \
	SCOOP='glab' \
	SNAP='glab' SNAP_CHANNEL='edge' \
	YAY='gitlab-glab-bin' \
	KISS='gitlab-glab' \
	APK='glab' \
	NIX='nixos.glab' \
	PORT='glab' \
	CURL='https://j.mp/glab-cli' BINDIR="$XDG_BIN_HOME" \
	setup-util
